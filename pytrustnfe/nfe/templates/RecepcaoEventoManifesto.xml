<envEvento xmlns="http://www.portalfiscal.inf.br/nfe" versao="1.00">
  <idLote>{{ evento.lote }}</idLote>
  <evento xmlns="http://www.portalfiscal.inf.br/nfe" versao="1.00">
    <infEvento Id="{{ evento.id }}">
      <cOrgao>{{ evento.orgao }}</cOrgao>
      <tpAmb>{{ ambiente }}</tpAmb>
      <CNPJ>{{ evento.cnpj }}</CNPJ>
      <chNFe>{{ evento.chave_nfe }}</chNFe>
      <dhEvento>{{ evento.data_hora_evento }}</dhEvento>
      <tpEvento>210200</tpEvento>
      <nSeqEvento>{{ evento.sequencia }}</nSeqEvento>
      <verEvento>1.00</verEvento>
      <detEvento versao="1.00">
        <descEvento>Confirmacao da Operacao</descEvento>
      </detEvento>
    </infEvento>
  </evento>
</envEvento>
